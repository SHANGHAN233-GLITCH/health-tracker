{
  "version": 2,
  "builds": [
    {
      "src": "build-web.js",
      "use": "@vercel/node",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "functions": {
    "build-web.js": {
      "memory": 1024
    }
  }
}